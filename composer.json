{
    "name":"martin-kozianka/contao-simpletipp",
    "description":"Simpletipp",
    "keywords":["contao", "openligadb", "tippspiel", "soccer", "game"],
    "type":"contao-module",
    "license":"LGPL-3.0+",
    "authors":[
        {
            "name":"Martin Kozianka",
            "email":"martin@kozianka.de",
            "homepage":"http://kozianka.de",
            "role":"Developer"
        }
    ],
    "support":{
        "email":"martin@kozianka.de",
        "issues":"https://github.com/kozi/simpletipp/issues",
        "irc":"irc://irc.freenode.org/contao.dev",
        "source":"https://github.com/kozi/simpletipp"

    },
    "require":{
        "php": ">=5.4",
        "contao/core": "~3.3",
        "contao-community-alliance/composer-plugin": "~2",
